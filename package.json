{
  "name": "muenchhausen-client",
  "version": "1.0.2",
  "description": "Shell client to generate huge quantities of authentic fake data for testing purposes or mockups.",
  "homepage": "https://github.com/s-a/muenchhausen-client",
  "author": {
    "name": "s-a",
    "email": "stephan.ahlf@googlemail.com",
    "url": "https://github.com/s-a"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "bin": {
    "mh": "lib/index.js"
  },
  "preferGlobal": true,
  "keywords": [
    "test",
    "testing",
    "testing-tools",
    "test-automation",
    "tests",
    "terminal",
    "bash",
    "shell",
    "console",
    "console-application",
    "console-tool",
    "mock",
    "mocking",
    "mockup",
    "fake",
    "fake-content",
    "fake-data"
  ],
  "devDependencies": {
    "eslint": "^3.1.1",
    "eslint-config-xo-space": "^0.16.0",
    "gulp": "^3.9.0",
    "gulp-coveralls": "^0.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-istanbul": "^1.0.0",
    "gulp-line-ending-corrector": "^1.0.1",
    "gulp-mocha": "^2.0.0",
    "gulp-nsp": "^2.1.0",
    "gulp-plumber": "^1.0.0",
    "mock-stdin": "^0.3.1",
    "should": "^11.2.1"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "repository": "s-a/muenchhausen-client",
  "scripts": {
    "prepublish": "gulp prepublish",
    "test": "gulp",
    "dev": "node lib/index.js \"$(date.month) $(date.weekday) $(emoji.icon name:fast_forward)\t$(emoji.random)\tnow:$(date.now)\trandom:$(date.random suffix:format,min:20200901)\trandom: $(date.random)\" --culture de-DE",
    "dev-pipe": "echo \"now:$(date.now)\" | node lib/index.js --culture es-ES",
    "dev-pipe-2": "echo \"now:$(date.now)\" | node lib/index.js --culture es-ES",
    "bump": "node node_modules/mocha/bin/_mocha && npm version patch && git push && git push --tags && npm publish"
  },
  "license": "MIT",
  "dependencies": {
    "get-stdin": "^5.0.1",
    "minimist": "^1.2.0",
    "muenchhausen": "^0.0.8",
    "n-cli": "^1.2.10"
  }
}
